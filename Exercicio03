Private Sub CommandButton6_Click()
Planilha1.Cells(1, 1) = "Dados 1" 'cabeçalho
Planilha1.Cells(1, 2) = "Dados 2" 'cabeçalho
Planilha1.Cells(1, 4) = "Resultado" 'cabeçalho
Dim matriz() As Single

Dim k As Integer

k = InputBox("Informe quantos dados serão preenchidos")

    For i = 2 To k + 1
        ReDim matriz(i, 1)
        matriz(i, 1) = InputBox("Informe o dado 1", "Dados", "0")
        Planilha1.Cells(i, 1) = matriz(i, 1)
    Next
End Sub

Private Sub CommandButton7_Click()

Planilha1.Cells(1, 1) = "Dados 1" 'cabeçalho
Planilha1.Cells(1, 2) = "Dados 2" 'cabeçalho
Planilha1.Cells(1, 4) = "Resultado" 'cabeçalho
Dim matriz() As Single

Dim k As Integer

k = InputBox("Informe quantos dados serão preenchidos")

    For i = 2 To k + 1
        ReDim matriz(i, 2)
        matriz(i, 2) = InputBox("Informe o dado 2", "Dados", "0")
        Planilha1.Cells(i, 2) = matriz(i, 2)
    Next
End Sub
